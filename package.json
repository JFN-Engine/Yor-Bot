    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.7",
    "@discordjs/voice": "^0.17.0",
    "@distube/spotify": "^1.6.1",
    "@distube/yt-dlp": "^1.1.3",
